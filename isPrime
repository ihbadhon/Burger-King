#include <iostream>
#include <vector>

using namespace std;

//Driver code
int main()
{
    long long int n,i;
    cin >> n;
    vector<bool>isPrime(n+1, true);

    for(i=2; i<=n; i++){
        if(isPrime[i]){
                if(i==n){
                    cout << "Yes" << endl;
                    break;
                }
            for(long long int j= i*i; j<=n; j+=i){
                isPrime[j] = false;
            }
        }
    }
if(i!=n){
    cout << "No" << endl;
}
    return 0;
}
